{
	"format_version": "1.10.0",
	"animation_controllers": {
		"controller.animation.morph.frog.general": {
			"initial_state": "on_land",
			"states": {
				"on_land": {
					"animations": [
						{ "frog_walk": "query.modified_move_speed" }
					],
					"transitions": [
						{ "idling_water": "query.is_in_water && query.modified_move_speed < 0.15" },
						{ "swimming": "query.is_in_water && query.modified_move_speed > 0.15" },
						{ "jumping": "query.is_powered" }
					],
					"blend_transition": 0.2
				},
				"jumping": {
					"animations": [
						{ "frog_jump": "query.is_powered" }
					],
					"transitions": [
						{ "on_land": "!query.is_powered && !query.is_in_water" },
						{ "idling_water": "!query.is_powered && query.is_in_water && query.modified_move_speed < 0.15" },
						{ "swimming": "!query.is_powered && query.is_in_water && query.modified_move_speed > 0.15" }
					],
					"blend_transition": 0.2
				},
				"idling_water": {
					"animations": [
						"frog_idle_water"
					],
					"transitions": [
						{ "on_land": "!query.is_in_water" },
						{ "swimming": "query.is_in_water && query.modified_move_speed > 0.15" }
					],
					"blend_transition": 0.2
				},
				"swimming": {
					"animations": [
						{ "frog_swim": "query.modified_move_speed" }
					],
					"transitions": [
						{ "on_land": "!query.is_in_water" },
						{ "idling_water": "query.is_in_water && query.modified_move_speed < 0.15" }
					],
					"blend_transition": 0.2
				}
			}
		},
		"controller.animation.morph.frog.attack": {
			"initial_state": "default",
			"states": {
				"default": {
					"transitions": [
						{ "attack": "variable.attack_time > 0.0" }
					]
				},
				"attack": {
					"animations": [ "frog_tongue" ],
					"transitions": [
						{ "default": "query.all_animations_finished" }
					]
				}
			}
		}
	}
}
