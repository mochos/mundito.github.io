{
	"format_version": "1.8.0",
	"render_controllers": {
		"controller.render.morph.goat.first_person": {
			"geometry": "Geometry.goat",
			"part_visibility": [
				{ "*": false },
				{ "right_front_leg": "query.get_equipped_item_name(0, 1) == '' || query.get_equipped_item_name(0, 1) == 'filled_map' || query.get_equipped_item_name(0) == 'morphing_bracelet'" },
				{ "left_front_leg": "(query.get_equipped_item_name(0, 1) == 'filled_map' && query.get_equipped_item_name('off_hand') != 'shield') || (query.get_equipped_item_name('off_hand') == 'filled_map' && !query.item_is_charged) || (!query.item_is_charged && (variable.item_use_normalized > 0 && variable.item_use_normalized < 1.0))" }
			],
			"materials": [ { "*": "Material.goat" } ],
			"textures": [ "Texture.goat" ]
		},
		"controller.render.morph.goat.third_person": {
			"geometry": "Geometry.goat",
			"part_visibility": [
				{ "right_horn": "!query.is_baby" },
				{ "left_horn": "!query.is_baby" }
			],
			"materials": [ { "*": "Material.goat" } ],
			"textures": [ "Texture.goat" ]
		}
	}
  }
  