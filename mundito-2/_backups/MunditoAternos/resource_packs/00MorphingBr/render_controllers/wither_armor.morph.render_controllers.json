{
  "format_version": "1.8.0",
  "render_controllers": {
    "controller.render.morph.wither_boss_armor_white": {
      "geometry": "Geometry.wither_armor",
      "materials": [ { "*": "Material.wither_armor" } ],
      "textures": [ "Texture.wither_armor_white" ],
      "part_visibility": [ { "*": "query.health <= 300" } ],
      "uv_anim": {
        "offset": [
          "(query.health <= 300 ? (Math.cos(query.life_time * 22.92) * 3) : 0.0)",
          "(query.health <= 300 ? (query.life_time * 0.2) : 0.0)"
        ],
        "scale": [ 1.0, 1.0 ]
      },
      "ignore_lighting": true
    },
    "controller.render.morph.wither_boss_armor_blue": {
      "geometry": "Geometry.wither_armor",
      "materials": [ { "*": "Material.wither_armor" } ],
      "textures": [ "Texture.wither_armor_blue" ],
      "part_visibility": [ { "*": "query.health <= 300" } ],
      "uv_anim": {
        "offset": [
          "(query.health <= 300) ? (Math.floor(query.life_time * 20.0) + query.frame_alpha) * 0.01 : 0.0",
          "(query.health <= 300) ? (Math.floor(query.life_time * 20.0) + query.frame_alpha) * 0.01 : 0.0"
        ],
        "scale": [ 1.0, 1.0 ]
      },
      "ignore_lighting": true
    }
  }
}
