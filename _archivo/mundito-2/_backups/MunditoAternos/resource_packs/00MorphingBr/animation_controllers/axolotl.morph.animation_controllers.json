{
  "format_version" : "1.10.0",
  "animation_controllers" : {
    "controller.animation.morph.axolotl.general": {
      "initial_state": "default",
      "states": {
        "default": {
          "animations": [
            {
              "axolotl_play_dead": "query.is_playing_dead"
            },
            {
              "axolotl_swim_angle": "(query.ground_speed > 0 || query.vertical_speed > 0) && query.is_in_water && !query.is_on_ground"
            }
          ]
        }
      }
    },
    "controller.animation.morph.axolotl.move": {
      "initial_state": "idle",
      "states": {
        "idle": {
          "animations": [
            {
              "axolotl_idle_float": "query.is_in_water && !query.is_on_ground && !query.is_playing_dead"
            },
            {
              "axolotl_idle_floor": "!query.is_in_water"
            },
            {
              "axolotl_idle_floor_water": "query.is_in_water && query.is_on_ground"
            }
          ],
          "transitions": [
            {
              "moving": "(query.ground_speed > 0 || query.vertical_speed > 0) && !query.is_playing_dead"
            }
          ]
        },
        "moving": {
          "animations": [
            {
              "axolotl_swim": "query.is_in_water && !query.is_on_ground"
            },
            {
              "axolotl_walk_floor": "!query.is_in_water"
            },
            {
              "axolotl_walk_floor_water": "query.is_in_water && query.is_on_ground"
            }
          ],
          "transitions": [
            {
              "idle": "!(query.ground_speed > 0 || query.vertical_speed > 0) || query.is_playing_dead"
            }
          ]
        }
      }
    }
  }
}
