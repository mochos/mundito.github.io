{
	"format_version" : "1.10.0",
	"animation_controllers" : {
		"controller.animation.morph.cat.move" : {
			"initial_state" : "walking",
			"states" : {
				"lie_down" : {
					"animations" : [ "cat_lie_down" ],
					"transitions" : [
						{
							"sneaking" : "query.is_moving && query.is_sneaking && !query.is_sleeping"
						},
						{
							"sprinting" : "query.is_sprinting && !query.is_sneaking && !query.is_sleeping"
						},
						{
							"sitting" : "!query.is_moving && query.is_sneaking && !query.is_sleeping"
						},
						{
							"walking" : "!query.is_sneaking && !query.is_sprinting && !query.is_sleeping"
						}
					]
				},
				"sitting" : {
					"animations" : [ "cat_sit" ],
					"transitions" : [
						{
							"sneaking" : "query.is_moving && query.is_sneaking && !query.is_sleeping"
						},
						{
							"sprinting" : "query.is_sprinting && !query.is_sneaking && !query.is_sleeping"
						},
						{
							"walking" : "!query.is_sneaking && !query.is_sprinting && !query.is_sleeping"
						},
						{
							"lie_down" : "query.is_sleeping"
						}
					]
				},
				"sneaking" : {
					"animations" : [ "cat_sneak" ],
					"transitions" : [
						{
							"sprinting" : "query.is_sprinting && !query.is_sneaking && !query.is_sleeping"
						},
						{
							"sitting" : "!query.is_moving && query.is_sneaking && !query.is_sleeping"
						},
						{
							"walking" : "!query.is_sneaking && !query.is_sprinting && !query.is_sleeping"
						},
						{
							"lie_down" : "query.is_sleeping"
						}
					]
				},
				"sprinting" : {
					"animations" : [ "cat_sprint" ],
					"transitions" : [
						{
							"sneaking" : "query.is_moving && query.is_sneaking && !query.is_sleeping"
						},
						{
							"sitting" : "!query.is_moving && query.is_sneaking && !query.is_sleeping"
						},
						{
							"walking" : "!query.is_sneaking && !query.is_sprinting && !query.is_sleeping"
						},
						{
							"lie_down" : "query.is_sleeping"
						}
					]
				},
				"walking" : {
					"animations" : [ "cat_walk" ],
					"transitions" : [
						{
							"sneaking" : "query.is_moving && query.is_sneaking && !query.is_sleeping"
						},
						{
							"sprinting" : "query.is_sprinting && !query.is_sneaking && !query.is_sleeping"
						},
						{
							"sitting" : "!query.is_moving && query.is_sneaking && !query.is_sleeping"
						},
						{
							"lie_down" : "query.is_sleeping"
						}
					]
				}
			}
		}
	}
}
