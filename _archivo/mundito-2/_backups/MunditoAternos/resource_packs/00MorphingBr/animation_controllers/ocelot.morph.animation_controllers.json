{
	"format_version" : "1.10.0",
	"animation_controllers" : {
		"controller.animation.morph.ocelot.move" : {
			"initial_state" : "walking",
			"states" : {
				"sitting" : {
					"animations" : [ "ocelot_sit" ],
					"transitions" : [
						{
							"sneaking" : "query.is_moving && query.is_sneaking"
						},
						{
							"sprinting" : "query.is_sprinting && !query.is_sneaking"
						},
						{
							"walking" : "!query.is_sneaking && !query.is_sprinting"
						}
					]
				},
				"sneaking" : {
					"animations" : [ "ocelot_sneak" ],
					"transitions" : [
						{
							"sprinting" : "query.is_sprinting && !query.is_sneaking"
						},
						{
							"sitting" : "!query.is_moving && query.is_sneaking"
						},
						{
							"walking" : "!query.is_sneaking && !query.is_sprinting"
						}
					]
				},
				"sprinting" : {
					"animations" : [ "ocelot_sprint" ],
					"transitions" : [
						{
							"sneaking" : "query.is_moving && query.is_sneaking"
						},
						{
							"sitting" : "!query.is_moving && query.is_sneaking"
						},
						{
							"walking" : "!query.is_sneaking && !query.is_sprinting"
						}
					]
				},
				"walking" : {
					"animations" : [ "ocelot_walk" ],
					"transitions" : [
						{
							"sneaking" : "query.is_moving && query.is_sneaking"
						},
						{
							"sprinting" : "query.is_sprinting && !query.is_sneaking"
						},
						{
							"sitting" : "!query.is_moving && query.is_sneaking"
						}
					]
				}
			}
		}
	}
}