{
  "format_version": "1.8.0",
  "render_controllers": {
    "controller.render.morph.mooshroom.first_person": {
      "arrays": {
        "textures": {
          "Array.skins": [
            "Texture.mooshroom_red",
            "Texture.mooshroom_brown"
          ]
        }
      },
      "geometry": "Geometry.mooshroom",
      "materials": [ { "*": "Material.mooshroom" } ],
      "textures": [ "Array.skins[query.mark_variant]" ],
      "part_visibility": [
        { "*": false },
        { "leg2": "query.get_equipped_item_name(0, 1) == '' || query.get_equipped_item_name(0, 1) == 'filled_map' || query.get_equipped_item_name(0) == 'morphing_bracelet'" },
        { "leg3": "(query.get_equipped_item_name(0, 1) == 'filled_map' && query.get_equipped_item_name('off_hand') != 'shield') || (query.get_equipped_item_name('off_hand') == 'filled_map' && !query.item_is_charged) || (!query.item_is_charged && (variable.item_use_normalized > 0 && variable.item_use_normalized < 1.0))" }
      ]
    },
    "controller.render.morph.mooshroom.third_person": {
      "arrays": {
        "textures": {
          "Array.skins": [
            "Texture.mooshroom_red",
            "Texture.mooshroom_brown"
          ]
        }
      },
      "geometry": "Geometry.mooshroom",
      "materials": [ { "*": "Material.mooshroom" } ],
      "textures": [ "Array.skins[query.mark_variant]" ]
    },
    "controller.render.morph.mooshroom_mushroom": {
      "arrays": {
        "textures": {
          "Array.skins": [
            "Texture.mushroom_red",
            "Texture.mushroom_brown"
          ]
        }
      },
      "geometry": "Geometry.mooshroom_mushroom",
      "materials": [ { "mushroom": "Material.mooshroom" } ],
      "textures": [ "Array.skins[query.mark_variant]" ],
      "part_visibility": [
        { "*": "!query.is_baby" }
      ]
    }
  }
}
