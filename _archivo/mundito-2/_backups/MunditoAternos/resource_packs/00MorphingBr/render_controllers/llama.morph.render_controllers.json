{
  "format_version": "1.8.0",
  "render_controllers": {
    "controller.render.morph.llama.first_person": {
      "arrays": {
        "textures": {
          "Array.base": [ "Texture.llama_creamy", "Texture.llama_white", "Texture.llama_brown", "Texture.llama_gray" ]
        }
      },
      "geometry": "Geometry.llama",
      "materials": [ { "*": "Material.llama" } ],
      "textures": [ "Array.base[query.mark_variant]" ],
      "part_visibility": [
        { "*": false },
        { "leg2": "query.get_equipped_item_name(0, 1) == '' || query.get_equipped_item_name(0, 1) == 'filled_map' || query.get_equipped_item_name(0) == 'morphing_bracelet'" },
        { "leg3": "(query.get_equipped_item_name(0, 1) == 'filled_map' && query.get_equipped_item_name('off_hand') != 'shield') || (query.get_equipped_item_name('off_hand') == 'filled_map' && !query.item_is_charged) || (!query.item_is_charged && (variable.item_use_normalized > 0 && variable.item_use_normalized < 1.0))" }
      ]
    },
    "controller.render.morph.llama.third_person": {
      "arrays": {
        "textures": {
          "Array.base": [ "Texture.llama_creamy", "Texture.llama_white", "Texture.llama_brown", "Texture.llama_gray" ]
        }
      },
      "geometry": "Geometry.llama",
      "part_visibility": [ { "chest*": "query.is_chested" } ],
      "materials": [ { "*": "Material.llama" } ],
      "textures": [ "Array.base[query.mark_variant]" ]
    }
  }
}
