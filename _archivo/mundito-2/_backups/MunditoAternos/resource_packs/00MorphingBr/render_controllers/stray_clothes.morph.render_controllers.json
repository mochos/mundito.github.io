{
  "format_version": "1.8.0",
  "render_controllers": {
    "controller.render.morph.stray_clothes.first_person": {
      "rebuild_animation_matrices": true,
      "geometry": "Geometry.stray_overlay",
      "materials": [ { "*": "Material.stray_overlay" } ],
      "textures": [ "Texture.stray_overlay" ],
      "part_visibility": [
        { "*": false },
        { "rightArm": "query.get_equipped_item_name(0, 1) == '' || query.get_equipped_item_name(0, 1) == 'map' || query.get_equipped_item_name(0) == 'morphing_bracelet'" },
        { "leftArm": "(query.get_equipped_item_name(0, 1) == 'map' && query.get_equipped_item_name('off_hand') != 'shield') || (query.get_equipped_item_name('off_hand') == 'map' && !query.item_is_charged) || (!query.item_is_charged && (variable.item_use_normalized > 0 && variable.item_use_normalized < 1.0))" }
      ]
    },
    "controller.render.morph.stray_clothes.third_person": {
      "rebuild_animation_matrices": true,
      "geometry": "Geometry.stray_overlay",
      "materials": [ { "*": "Material.stray_overlay" } ],
      "textures": [ "Texture.stray_overlay" ]
    }
  }
}