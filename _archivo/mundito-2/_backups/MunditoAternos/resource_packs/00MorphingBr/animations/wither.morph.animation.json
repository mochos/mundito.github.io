{
	"format_version": "1.8.0",
	"animations": {
		"animation.morph.wither_boss.look_at_target" : {
			"loop" : true,
			"bones" : {
				"head1" : {
					"rotation" : [ "query.target_x_rotation - this", "query.target_y_rotation - this", "-this" ]
				},
				"head2" : {
					"rotation" : [ "(query.target_x_rotation - 5) - this", "(query.target_y_rotation - 5) - this", "-this" ]
				},
				"head3" : {
					"rotation" : [ "(query.target_x_rotation - 5) - this", "(query.target_y_rotation + 5) - this", "-this" ]
				}
			}
		}
	}
}