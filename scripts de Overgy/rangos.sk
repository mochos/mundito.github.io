command /rango <text> <player> [<number>]:
	executable by: players and console
	permission: skript.admin
	trigger:
		if arg 3 is not set:
			set {_tiempo} to 30
		else:
			set {_tiempo} to arg 3
		if arg 1 is "lord":
			message "&aActivando rango..." to arg 2
			execute console command "/pex user %arg 2% group add lord"
			wait 3 seconds
			execute console command "/pex user %arg 2% group remove default"
			execute console command "/pex user %arg 2% group remove rey"
			execute console command "/pex user %arg 2% group remove erudito"
			execute console command "/pex user %arg 2% group remove deidad"
			wait 3 seconds
			if {_tiempo} is less than 30:
				message "&fEste rango es de prueba y viene sin kits, dinero y experiencia." to arg 2
			else:
				execute console command "/kit lord %arg 2%"
				execute console command "/money give %arg 2% 100000"
				execute console command "/exp give %arg 2% 20L"
			wait 3 seconds
			execute console command "/pex reload"
			set yaml value "rangos.%arg 2%.inicio" from file "../variables/rangos.yml" to "%now%"
			set yaml value "rangos.%arg 2%.tiempo" from file "../variables/rangos.yml" to {_tiempo}
			set yaml value "rangos.%arg 2%.rango" from file "../variables/rangos.yml" to arg 1
			set yaml value "rangos.%arg 2%.restante" from file "../variables/rangos.yml" to {_tiempo}*24
			add "[%now%] %arg 2% activó %arg 1% por %{_tiempo}% días" to yaml list "registro" from file "../variables/rangos.yml"
			message "&fFelicidades por tu nuevo rango &6&lLORD&f por %{_tiempo}% días." to arg 2
			stop 
		if arg 1 is "rey":
			message "&aActivando rango..." to arg 2
			execute console command "/pex user %arg 2% group add rey"
			wait 3 seconds
			execute console command "/pex user %arg 2% group remove default"
			execute console command "/pex user %arg 2% group remove lord"
			execute console command "/pex user %arg 2% group remove erudito"
			execute console command "/pex user %arg 2% group remove deidad"
			wait 3 seconds
			if {_tiempo} is less than 30:
				message "&fEste rango es de prueba y viene sin kits, dinero y experiencia." to arg 2
			else:
				execute console command "/kit rey %arg 2%"
				execute console command "/money give %arg 2% 200000"
				execute console command "/exp give %arg 2% 30L"
			wait 3 seconds
			execute console command "/pex reload"
			set yaml value "rangos.%arg 2%.inicio" from file "../variables/rangos.yml" to "%now%"
			set yaml value "rangos.%arg 2%.tiempo" from file "../variables/rangos.yml" to {_tiempo}
			set yaml value "rangos.%arg 2%.rango" from file "../variables/rangos.yml" to arg 1
			set yaml value "rangos.%arg 2%.restante" from file "../variables/rangos.yml" to {_tiempo}*24
			add "[%now%] %arg 2% activó %arg 1% por %{_tiempo}% días" to yaml list "registro" from file "../variables/rangos.yml"
			message "&fFelicidades por tu nuevo rango &e&lREY&f." to arg 2
			stop
		if arg 1 is "erudito":
			message "&aActivando rango..." to arg 2
			execute console command "/pex user %arg 2% group add erudito"
			wait 3 seconds
			execute console command "/pex user %arg 2% group remove default"
			execute console command "/pex user %arg 2% group remove rey"
			execute console command "/pex user %arg 2% group remove lord"
			execute console command "/pex user %arg 2% group remove deidad"
			wait 3 seconds
			if {_tiempo} is less than 30:
				message "&fEste rango es de prueba y viene sin kits, dinero y experiencia." to arg 2
			else:
				execute console command "/kit erudito %arg 2%"
				execute console command "/money give %arg 2% 400000"
				execute console command "/exp give %arg 2% 40L"
			wait 3 seconds
			execute console command "/pex reload"
			set yaml value "rangos.%arg 2%.inicio" from file "../variables/rangos.yml" to "%now%"
			set yaml value "rangos.%arg 2%.tiempo" from file "../variables/rangos.yml" to {_tiempo}
			set yaml value "rangos.%arg 2%.rango" from file "../variables/rangos.yml" to arg 1
			set yaml value "rangos.%arg 2%.restante" from file "../variables/rangos.yml" to {_tiempo}*24
			add "[%now%] %arg 2% activó %arg 1% por %{_tiempo}% días" to yaml list "registro" from file "../variables/rangos.yml"
			message "&fFelicidades por tu nuevo rango &d&lERUDITO&f." to arg 2
			stop
		if arg 1 is "deidad":
			message "&aActivando rango..." to arg 2
			execute console command "/pex user %arg 2% group add deidad"
			wait 3 seconds
			execute console command "/pex user %arg 2% group remove default"
			execute console command "/pex user %arg 2% group remove rey"
			execute console command "/pex user %arg 2% group remove erudito"
			execute console command "/pex user %arg 2% group remove lord"
			wait 3 seconds
			if {_tiempo} is less than 30:
				message "&fEste rango es de prueba y viene sin kits, dinero y experiencia." to arg 2
			else:
				execute console command "/kit deidad %arg 2%"
				execute console command "/money give %arg 2% 1000000"
				execute console command "/exp give %arg 2% 50L"
			wait 3 seconds
			execute console command "/pex reload"
			set yaml value "rangos.%arg 2%.inicio" from file "../variables/rangos.yml" to "%now%"
			set yaml value "rangos.%arg 2%.tiempo" from file "../variables/rangos.yml" to {_tiempo}
			set yaml value "rangos.%arg 2%.rango" from file "../variables/rangos.yml" to arg 1
			set yaml value "rangos.%arg 2%.restante" from file "../variables/rangos.yml" to {_tiempo}*24
			add "[%now%] %arg 2% activó %arg 1% por %{_tiempo}% días" to yaml list "registro" from file "../variables/rangos.yml"
			message "&fFelicidades por tu nuevo rango &c&lDEIDAD&f." to arg 2
			stop

#Consulta rango propio o de otro
command /consulta [<offline player>]:
	executable by: players and console
	permission: skript.admin
	trigger:
		if arg 1 is not set:
			set {_jugador} to player
			set {_rango} to yaml value "rangos.%player%.rango" from file "../variables/rangos.yml"
			set {_tiempo} to yaml value "rangos.%player%.tiempo" from file "../variables/rangos.yml"
			set {_inicio} to yaml value "rangos.%player%.inicio" from file "../variables/rangos.yml"
			set {_restante} to yaml value "rangos.%player%.restante" from file "../variables/rangos.yml"
			if {_rango} is set:
				message "&6&lInformación de tu rango" to player
				message " &aRango: &f%{_rango}%" to player
				message " &aDuracion: &f%{_tiempo}% días" to player
				message " &aInicio: &f%{_inicio}%" to player
				message " &aRestante: &f%{_restante}% horas" to player
				stop
			else:
				message " &cNo tienes ningún rango activo en el momento." to player
				stop
		else:
			if player has permission "skript.admin":
				set {_jugador} to arg 1
				set {_rango} to yaml value "rangos.%arg 1%.rango" from file "../variables/rangos.yml"
				set {_tiempo} to yaml value "rangos.%arg 1%.tiempo" from file "../variables/rangos.yml"
				set {_inicio} to yaml value "rangos.%arg 1%.inicio" from file "../variables/rangos.yml"
				set {_restante} to yaml value "rangos.%arg 1%.restante" from file "../variables/rangos.yml"
				wait 1 tick
				if {_rango} is set:
					message "&6&lRango de %{_jugador}%" to player
					message " &aRango: &f%{_rango}%" to player
					message " &aDuracion: &f%{_tiempo}% días" to player
					message " &aInicio: &f%{_inicio}%" to player
					message " &aRestante: &f%{_restante}% horas" to player
					stop
				else:
					message " &c%{_jugador}% no tiene ningún rango activo en el momento." to player
					stop
			else:
				message " &cNo tienes permiso para hacer consultas de otro jugador." to player
				stop

#Cada hora resta una hora a la al nodo restante de cada jugador
every 3599 seconds: #3599
	set {_milista::*} to yaml nodes "rangos" from "../variables/rangos.yml"
	loop {_milista::*}:
		set {_conteo} to yaml value "rangos.%{_milista::%loop-index%}%.restante" from file "../variables/rangos.yml"
		if {_conteo} is greater than 1:
			remove 1 from {_conteo}
			wait 1 tick
			set yaml value "rangos.%{_milista::%loop-index%}%.restante" from file "../variables/rangos.yml" to {_conteo}
		if {_conteo} is less than 1.1:
			execute console command "/pex user %{_milista::%loop-index%}% group remove lord"
			execute console command "/pex user %{_milista::%loop-index%}% group remove rey"
			execute console command "/pex user %{_milista::%loop-index%}% group remove erudito"
			execute console command "/pex user %{_milista::%loop-index%}% group remove deidad"
			wait 3 seconds
			execute console command "/pex reload"
			wait 1 tick
			add "[%now%] %{_milista::%loop-index%}% finalizó su rango." to yaml list "registro" from file "../variables/rangos.yml"
			delete yaml value "rangos.%{_milista::%loop-index%}%" from "../variables/rangos.yml"
	stop

